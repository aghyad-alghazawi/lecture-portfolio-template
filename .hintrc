{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "animation-timeline",
          "animation-timeline: view()",
          "animation-range",
          "animation-timeline: scroll()",
          "text-wrap: pretty"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ]
  }
}