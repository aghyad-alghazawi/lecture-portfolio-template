{
  "extends": ["development"],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-wrap",
          "text-wrap: balance",
          "text-wrap: pretty",
          "animation-timeline",
          "animation-timeline: view()",
          "animation-range",
          "animation-timeline: scroll()"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ]
  }
}
